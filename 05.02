function average(array) {
  function plus(a, b) { return a + b; }
  return array.reduce(plus) / array.length;
}
 
var byName = {};
ancestry.forEach(function(person) {
  byName[person.name] = person;
});

var withMother = ancestry.filter(function(person) {
return person.mother !== null ;
});

var haveMotherInAncestry = withMother.filter(function (person) { 
return byName[person.mother] });

var mothers = haveMotherInAncestry.map(function (person) { 
return byName[person.mother] })

var mothersBorn = mothers.map(function (person) {
return person.born;})

var childBorn = haveMotherInAncestry.map(function (person) { 
return person.born; });

function forEach (mother, child) {
  var diff = [];
  for (var i = 0; i<mother.length; i++) {
  	diff.push(child[i]-mother[i]);
  }
  return diff;
 
}
var difference = forEach(mothersBorn, childBorn);
 
console.log(average(difference));
